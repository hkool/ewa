{% extends 'base.html.twig' %}

{% block title %}Partner{% endblock %}

{% block body %}
    <h1>Partner</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partner.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ partner.name }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ partner.website }}</td>
            </tr>
            <tr>
                <th>Img</th>
                <td>{{ partner.img }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ partner.created ? partner.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ partner.updated ? partner.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('partner_index') }}">back to list</a>

    <a href="{{ path('partner_edit', {'id': partner.id}) }}">edit</a>

    {{ include('partner/_delete_form.html.twig') }}
{% endblock %}
