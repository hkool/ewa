{%  extends 'base.html.twig' %}
  {% block head %}
      <link rel="stylesheet" type="text/css" href="slick/slick.css"/>
      <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
      <link rel="stylesheet" type="text/css" href="slick/slick-theme.css"/>
      <link rel="stylesheet" type="text/css" href="ewa.css"/>
  {% endblock %}
{% block headerintro %}

<section class="nieuwsOverzicht">
    <div class="container text-center" >
        <div class="title">
            <div class="row justify-center-md-center">
                <div class="col-md-12 col-lg-12 col-sm-6 ">
                    <h2>PARTNERS</h2>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="container" >
    <section class="partners">
        <div class="row justify-center-md-center">
            {% for partner in partners %}
                    <!--    start  partners -->
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 ">
                   <div class="container">
                        <div class="card">
                            <div class="card-img"> <img class="card-img-top" src="images/logo/{{ partner.img }}" alt="Card image cap" /></div>
                                 <div class="card-body" background-image="url('images/logo{{ partner.img }}')">

                                     <h5>{{ partner.name }}</h5>
                                     <a href="{{ partner.website }}"><button >Website</button></a>


                                 </div>
                        </div>
                    </div>
                </div>
            <!--    einde  partners -->
            {% endfor %}
        </div>
    </section>

</div>
</section>
{% endblock %}




